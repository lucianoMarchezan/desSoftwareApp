<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
	<managed-bean>
	<managed-bean-name>experiment</managed-bean-name>
	<managed-bean-class>SoftwareDev.ExMAp.Controller.ExperimentBean
	</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean>
	<navigation-rule>
		<from-view-id></from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>new_experiment</from-outcome>
			<to-view-id>new_experiment.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>update_experiment</from-outcome>
			<to-view-id>update_experiment.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>